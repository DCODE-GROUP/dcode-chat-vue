{
  "name": "@dcodegroup-au/dcode-chat-vue",
  "version": "0.0.4",
  "description": "Frontend vue components for the dcode-chat package",
  "main": ".eslintrc.js",
  "keywords": [
    "vue",
    "tailwind",
    "dsg",
    "untitledui",
    "dcode-chat",
    "dcode"
  ],
  "files": ["dist"],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall": "npx only-allow pnpm",
    "preupdate": "npx --yes only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "serve": "vite preview",
    "lint": "eslint --flag unstable_ts_config src/",
    "lint:fix": "eslint --flag unstable_ts_config --fix src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DCODE-GROUP/laravel-chat-vue.git"
  },
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">= 9.0.0",
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/dcode-chat.esm.js",
      "require": "./dist/dcode-chat.cjs",
      "default": "./dist/dcode-chat.esm.js"
    },
    "./dcode-chat.css": "./dist/dcode-chat.css"
  },
  "peerDependencies": {
    "vue": "^3.1.0"
  },
  "dependencies": {
    "@dcodegroup-au/dsg-vue": "^0.0.13-alpha.6",
    "axios": "^1.9.0",
    "mitt": "^3.0.1",
    "ziggy": "^2.4.0",
    "ziggy-js": "^2.5.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "vite": "^6.3.5"
  }
}
